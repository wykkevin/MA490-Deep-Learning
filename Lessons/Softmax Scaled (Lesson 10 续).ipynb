    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def fsmax(x):\n",
    "    k=x.max()\n",
    "    e = np.exp(x-k)\n",
    "    p = e/e.sum()\n",
    "    return p"
   ]
  },
